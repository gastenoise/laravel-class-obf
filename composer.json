{
    "name": "your-vendor/laravel-class-obfuscator",
    "description": "A Laravel-compatible runtime class obfuscation library for PHP and JS bindings.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "obfuscator",
        "security",
        "php",
        "javascript",
        "classes",
        "runtime",
        "encryption"
    ],
    "authors": [
        {
            "name": "GastÃ³n",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "ClassObfuscator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClassObfuscator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ClassObfuscator\\ClassObfuscatorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "lint": "php -l src"
    }
}